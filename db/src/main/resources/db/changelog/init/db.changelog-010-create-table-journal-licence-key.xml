<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.8.xsd">

    <changeSet id="10" author="a.kakushin">
        <createTable tableName="journal_licence_key">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>

            <column name="created" type="timestamp"/>
            <column name="operation" type="UUID">
                <constraints unique="true" nullable="false"/>
            </column>

            <column name="data_processing" type="varchar(255)"/>
            <column name="protected_module" type="varchar(255)"/>
            <column name="file_name_for_download" type="varchar(255)"/>
            <column name="size" type="bigint"/>

        </createTable>
    </changeSet>
</databaseChangeLog>